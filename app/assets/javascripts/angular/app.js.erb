#= depend_on_asset "home.html"

angular
    .module('desidancevids', [
        'ngRoute',
        'templates'
    ]).config(function ($routeProvider, $locationProvider) {
        $routeProvider
            .when('/', {
                templateUrl: <%= asset_path('home.html') %>,
                controller: 'HomeCtrl'
            });
        $locationProvider.html5Mode(true);
    });